---
import Typography from "./Typography.astro"

interface Props {
	title: string
}

const { title } = Astro.props
---

<article class="z-20 flex min-h-[200px] items-center justify-start px-5 py-10">
	<header class="text-white">
		<div class="title-intro-page">
			<Typography as="h1" variant="h1" color="white">{title}</Typography>
		</div>
	</header>
</article>
